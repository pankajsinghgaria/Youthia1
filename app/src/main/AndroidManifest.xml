<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.youthia.youthia">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ActivityClass.LoginScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ActivityClass.HomeActivity" />
        <activity android:name=".ActivityClass.AdminLogin" />
        <activity android:name=".ActivityClass.AdminPage" />
        <activity android:name=".ActivityClass.AdminUserList" />
        <activity android:name=".ActivityClass.AdminUserList"></activity>
    </application>

</manifest>